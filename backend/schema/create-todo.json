{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "create-todo",
    "type": "object",
    "properties": {
      "createdAt": {
        "type": "string"
      },
      "name": {
        "type": "string",
        "minLength": 1
      },
      "dueDate": {
        "type": "string",
        "minLength": 1
      },
      "done": {
        "type": "boolean"
      }
    },
    "required": [
      "name", "dueDate"
    ],
    "additionalProperties": false
}